{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/contacts.json",
      "destination": "/contacts.json"
    },
    {
      "source": "/photos/(.*)",
      "destination": "/photos/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}